inir -state+\r\n    const [error, guardarError] = useState(false)\r\n    //Cuando el usuario hace clic en calcular\r\n    const calcularPrestamo =e=>{\r\n        e.preventDefault();\r\n\r\n        //Validar\r\n        if(Cantidad === 0 || Plazo === \"\") {\r\n\r\n            guardarError (true);\r\n            return;\r\n        }\r\n            //eliminar error\r\n            guardarError(false);\r\n\r\n        //habilitar Spiner\r\n            guardarCargando(true)\r\n        setTimeout(() => {\r\n             //realizar cotizacion\r\n            const Total = calcularTotal(Cantidad, Plazo)\r\n\r\n            //una vez calculado guadar total\r\n            guardarTotal (Total);  \r\n\r\n            //desabilitar spiner\r\n            guardarCargando(false)\r\n        }, 3000);\r\n      \r\n\r\n        \r\n\r\n    }\r\n   \r\n    return (  \r\n        \r\n        <Fragment>\r\n             <form onSubmit={calcularPrestamo}>\r\n                    <div className=\"row\">\r\n                        <div>\r\n                            <label>Cantidad Prestamo</label>\r\n                            <input \r\n                                className=\"u-full-width\" \r\n                                type=\"number\" \r\n                                placeholder=\"Ejemplo: 3000\" \r\n                                onChange={ e=>guardarCantidad(parseInt(e.target.value)) }\r\n                            />\r\n                        </div>\r\n                        <div>\r\n                            <label>Plazo para Pagar</label>\r\n                            <select \r\n                                className=\"u-full-width\"\r\n                                onChange={ e=>guardarPlazo(parseInt(e.target.value)) }\r\n                            >\r\n                                <option value=\"\">Seleccionar</option>\r\n                                <option value=\"3\">3 meses</option>\r\n                                <option value=\"6\">6 meses</option>\r\n                                <option value=\"12\">12 meses</option>\r\n                                <option value=\"24\">24 meses</option>\r\n                            </select>\r\n                        </div>\r\n                        <div>\r\n                            <input \r\n                                type=\"submit\" \r\n                                value=\"Calcular\" \r\n                                className=\"button-primary u-full-width\" \r\n                            />\r\n                        </div>\r\n                    </div>\r\n            </form>\r\n            {(error) ?  <p className= \"error\">Todos los campos son obligatorios</p> : null}\r\n           \r\n        </Fragment>\r\n    );\r\n    \r\n}\r\n \r\nexport default Formulario;","import React from 'react';\r\n\r\nconst Mensaje = () => (\r\n    <p>Agrega una Cantidad y Plazo a pagar para cotizar</p>\r\n)\r\n \r\nexport default Mensaje;","import React from 'react'\r\n\r\nconst Resutaldo = ({Total, Plazo, Cantidad}) => (\r\n    <div className = \"u-full-width resultado\">\r\n        <h2>Resumen</h2>\r\n        <p>La cantidad solicitada es : $ {Cantidad}</p>\r\n        <p>A pagar en {Plazo} meses</p>\r\n        <p>Su pago mensual es de : $ {(Total/Plazo).toFixed(2)}</p>\r\n        <p>Total a pagar : & {Total}</p>\r\n\r\n    </div>\r\n\r\n        \r\n );\r\n \r\nexport default Resutaldo;","import React from 'react';\r\nimport \"./Spiner.css\"\r\n\r\nconst Spiner = () => {\r\n    return (  \r\n\r\n<div className=\"sk-chase\">\r\n        <div className=\"sk-chase-dot\"></div>\r\n        <div className=\"sk-chase-dot\"></div>\r\n        <div className=\"sk-chase-dot\"></div>\r\n        <div className=\"sk-chase-dot\"></div>\r\n        <div className=\"sk-chase-dot\"></div>\r\n        <div className=\"sk-chase-dot\"></div>\r\n</div>\r\n    );\r\n}\r\n \r\nexport default Spiner;","import React, {Fragment, useState}  from 'react';\nimport './App.css';\nimport Header f